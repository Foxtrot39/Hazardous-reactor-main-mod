  <Item name="" identifier="fuelbackpack" tags="largeitem,mobilecontainer,hazardcontainement" description="" showcontentsintooltip="true" scale="0.5" linkable="true" pickdistance="150" impactsoundtag="impact_metal_heavy" waterproof="true" fireproof="true">

    <Fabricate suitablefabricators="fabricator" requiredtime="60">
